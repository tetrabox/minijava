<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
  <extension point="org.eclipse.gemoc.gemoc_language_workbench.xdsml">
    <XDSML_Definition name="org.tetrabox.minijava.XMiniJava" xdsmlFilePath="platform:/plugin/org.tetrabox.minijava.xminijava/model/XMiniJava.dsl" modelLoader_class="org.eclipse.gemoc.executionframework.extensions.sirius.modelloader.DefaultModelLoader" />
  </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated XMiniJava -->
      <package
            uri="http://org.tetrabox.minijava.xminijava/miniJava/"
            class="org.tetrabox.minijava.model.miniJava.MiniJavaPackage"
            genModel="model/XMiniJava.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated XMiniJava -->
      <package
            uri="http://org.tetrabox.minijava.xminijava/miniJava/semantics"
            class="org.tetrabox.minijava.model.miniJava.semantics.SemanticsPackage"
            genModel="model/XMiniJava.genmodel"/>
   </extension>

</plugin>
